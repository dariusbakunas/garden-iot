{
  "name": "garden-iot",
  "version": "0.0.1",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"nodemon\" \"yarn start:client\"",
    "build": "rm -rf ./build-server/ && yarn run build:server && yarn run build:client",
    "build:server": "tsc --project ./tsconfig.json",
    "build:client": "cd client && yarn build && cp -R ./build/ ../build-server/client && rm -rf ./build",
    "start:client": "cd client && yarn start",
    "start": "node --max-http-header-size=80000 build-server/index.js",
    "postinstall": "cd client && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dariusbakunas/garden-iot.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dariusbakunas/garden-iot/issues"
  },
  "homepage": "https://github.com/dariusbakunas/garden-iot#readme",
  "devDependencies": {
    "@types/carbon-components-react": "^7.10.8",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.48",
    "@types/node": "^14.6.0",
    "@types/rpio": "^0.0.3",
    "@types/socket.io": "^2.1.11",
    "@types/ws": "^7.2.6",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "node-webcam": "^0.6.4",
    "rpio": "^2.2.0",
    "socket.io": "^2.3.0"
  }
}
